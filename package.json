{
  "name": "r3test",
  "version": "0.3.0-EXPERIMENTAL",
  "description": "Sample/test project for react-three",
  "keywords": [
    "react",
    "threejs",
    "webGL",
    "canvas"
  ],
  "author": {
    "name": "Gary Haussmann",
    "email": "gjhaussmann@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Izzimach/r3test"
  },
  "license": "Apache-2.0",
  "main": "r3test.js",
  "dependencies": {
    "fbjs": "^0.8.0",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.2",
    "react-three": "^0.9.6",
    "redux": "^3.4.0",
    "three": "~0.74.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.0",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-react": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.6.1",
    "eslint": "^2.7.0",
    "eslint-plugin-react": "^4.3.0",
    "lodash": "^4.0.0",
    "react-hot-loader": "^1.3.0",
    "redux-thunk": "^2.0.0",
    "webpack": "^1.12.1",
    "webpack-dev-server": "^1.10.1"
  },
  "scripts": {
    "build": "webpack",
    "lint": "eslint src",
    "dev": "node devServer.js"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "ecmaFeatures": {
      "spread": true,
      "jsx": true
    },
    "rules": {
      "strict": [
        0
      ],
      "react/jsx-no-undef": 1,
      "react/jsx-no-duplicate-props": 1
    }
  },
  "engines": {
    "node": ">=0.12.0"
  }
}
